<PropertyList>
  <path>cockpit.ac</path>
  <!-- Suppress/Hide Mono Cockpit Models -->
  <animation>
    <type>select</type>
    <object-name>AD_LOD_100_M</object-name>
    <object-name>MonoBox</object-name>
    <condition>
      <property>/sim/model/mono-cockpit-enabled</property>
    </condition>
  </animation>
  <animation>
    <type>select</type>
    <object-name>AD_LOD_100</object-name>
    <condition>
      <not>
        <property>/sim/model/mono-cockpit-enabled</property>
      </not>
    </condition>
  </animation>
  <!-- Animate starter swtich -->
  <animation>
    <type>pick</type>
    <object-name>CZ_LOD_100</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <condition>
          <not>
            <property>/sim/model/chocks-enabled</property>
          </not>
        </condition>
        <command>property-toggle</command>
        <property>/controls/engines/engine[0]/starter</property>
      </binding>
      <binding>
        <command>nasal</command>
        <script>
          if (getprop("/sim/model/chocks-enabled"))
            gui.popupTip("Remove chocks and protections before starting the engine", 5, nil);
        </script>
      </binding>
    </action>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>CZ_LOD_100</object-name>
    <property>/controls/engines/engine[0]/starter</property>
    <factor>45</factor>
    <center>
      <x-m>-0.98869</x-m>
      <y-m>0.29478</y-m>
      <z-m>0.05375</z-m>
    </center>
    <axis>
      <y>-1</y>
    </axis>
  </animation>
  <!-- Animate fuel pump switch -->
  <animation>
    <type>pick</type>
    <object-name>DA_LOD_100</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/controls/engines/engine[0]/fuel-pump</property>
      </binding>
    </action>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>DA_LOD_100</object-name>
    <property>/controls/engines/engine[0]/fuel-pump</property>
    <factor>45</factor>
    <center>
      <x-m>-0.98869</x-m>
      <y-m>0.33226</y-m>
      <z-m>0.05375</z-m>
    </center>
    <axis>
      <y>-1</y>
    </axis>
  </animation>
  <!-- Animate fuel cuttof switch -->
  <animation>
    <type>pick</type>
    <object-name>DE_LOD_100</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <condition>
          <property>/sim/input/selected/engine[0]</property>
        </condition>
        <command>property-toggle</command>
        <property>/controls/engines/engine[0]/cutoff</property>
      </binding>
    </action>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>DE_LOD_100</object-name>
    <property>/controls/engines/engine[0]/cutoff</property>
    <offset-deg>-55</offset-deg>
    <factor>55</factor>
    <center>
      <x-m>-0.59968</x-m>
      <y-m>0.29196</y-m>
      <z-m>0.05266</z-m>
    </center>
    <axis>
      <y>1</y>
    </axis>
  </animation>
  <!-- Animate fuel cutoff switch cover -->
  <animation>
    <type>pick</type>
    <object-name>DF_LOD_100</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/controls/switches/cockpit/fuel-cutoff-cover</property>
      </binding>
    </action>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>DF_LOD_100</object-name>
    <property>/controls/switches/cockpit/fuel-cutoff-cover</property>
    <factor>45</factor>
    <center>
      <x-m>-0.62342</x-m>
      <y-m>0.29394</y-m>
      <z-m>0.05797</z-m>
    </center>
    <axis>
      <y>-1</y>
    </axis>
  </animation>
  <!-- Animate throttle lever -->
  <animation>
    <type>pick</type>
    <object-name>BP_LOD_100</object-name>
    <action>
      <button>3</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/controls/engines/engine[0]/throttle</property>
        <step>.1</step>
        <max>1</max>
      </binding>
    </action>
    <action>
      <button>4</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/controls/engines/engine[0]/throttle</property>
        <step>-.1</step>
        <min>0</min>
      </binding>
    </action>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>BP_LOD_100</object-name>
    <object-name>BQ_LOD_100</object-name>
    <property>/controls/engines/engine[0]/throttle</property>
    <offset-deg>24</offset-deg>
    <factor>-25</factor>
    <center>
      <x-m>-0.78766</x-m>
      <y-m>0.33057</y-m>
      <z-m>0.08606</z-m>
    </center>
    <axis>
      <z>1</z>
      <y>1</y>
    </axis>
  </animation>
  <!-- Animate center stick -->
  <animation>
    <type>rotate</type>
    <object-name>BW_LOD_100</object-name>
    <property>/controls/flight/aileron</property>
    <offset-deg>-12.5</offset-deg>
    <factor>-11</factor>
    <center>
      <x-m>-0.95377</x-m>
      <y-m>0.01844</y-m>
      <z-m>-0.23498</z-m>
    </center>
    <axis>
      <x>1</x>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>BW_LOD_100</object-name>
    <property>/controls/flight/elevator</property>
    <offset-deg>0</offset-deg>
    <factor>-12</factor>
    <center>
      <x-m>-0.95377</x-m>
      <y-m>0.01844</y-m>
      <z-m>-0.23498</z-m>
    </center>
    <axis>
      <y>1</y>
    </axis>
  </animation>
  <!-- Animate gear lever -->
  <animation>
    <type>pick</type>
    <object-name>BS_LOD_100</object-name>
    <action>
      <button>3</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>
          if (!getprop("/engines/engine[0]/running"))
            gui.popupTip("Cant retract gear without engine running. Not enough electrical power", 5, nil);
          else if (getprop('/sim/failure-manager/controls/gear/serviceable'))
            controls.gearDown(-1);
        </script>
      </binding>
    </action>
    <action>
      <button>4</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>
          if (getprop("/velocities/airspeed-kt") >= 194)
            gui.popupTip("Cant release gear above the maximum release speed", 5, nil);
          else if (getprop('/sim/failure-manager/controls/gear/serviceable'))
            controls.gearDown(1);
        </script>
      </binding>
    </action>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>BS_LOD_100</object-name>
    <property>/controls/gear/gear-down</property>
    <offset-deg>0</offset-deg>
    <factor>45</factor>
    <center>
      <x-m>-1.13409</x-m>
      <y-m>0.28597</y-m>
      <z-m>0.11855</z-m>
    </center>
    <axis>
      <y>.2</y>
      <x>.01</x>
    </axis>
  </animation>
  <!-- Animate emergency gear switch -->
  <animation>
    <type>pick</type>
    <object-name>gear_emerg_switch</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>
          gui.popupTip("Gear released in emergency mode", 5, nil);
          controls.gearDown(1);
          setprop('/sim/failure-manager/controls/gear/serviceable', 0);
        </script>
      </binding>
    </action>
  </animation>
  <!-- Animate gear status lights -->
  <animation>
    <type>select</type>
    <object-name>gear_status_light_g</object-name>
    <condition>
      <and>
        <equals>
          <property>/gear/gear[0]/position-norm</property>
          <value>1</value>
        </equals>
        <property>/sim/failure-manager/controls/gear/serviceable</property>
      </and>
    </condition>
  </animation>
  <animation>
    <type>select</type>
    <object-name>gear_status_light_r</object-name>
    <condition>
      <not>
        <property>/sim/failure-manager/controls/gear/serviceable</property>
      </not>
    </condition>
  </animation>
  <animation>
    <type>select</type>
    <object-name>gear_status_light</object-name>
    <condition>
      <and>
        <not>
          <equals>
            <property>/gear/gear[0]/position-norm</property>
            <value>1</value>
          </equals>
        </not>
        <property>/sim/failure-manager/controls/gear/serviceable</property>
      </and>
    </condition>
  </animation>
  <!-- Animate flap lever -->
  <animation>
    <type>pick</type>
    <object-name>BR_LOD_100</object-name>
    <action>
      <button>3</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>
          if (!getprop("/engines/engine[0]/running"))
            gui.popupTip("Cant use flaps without engine running. Not enough electrical power", 5, nil);
          else
            controls.flapsDown(-1);
        </script>
      </binding>
    </action>
    <action>
      <button>4</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>
          var current_speed = getprop("/velocities/airspeed-kt");
          var flap_pos = getprop("/controls/flight/flaps");
          if (!getprop("/engines/engine[0]/running")){
            gui.popupTip("Cant use flaps without engine running. Not enough electrical power", 5, nil);
            return;
          }
          else if ( (flap_pos == 0 and current_speed >= 212) or (flap_pos == 0.49 and current_speed >= 183) )
            gui.popupTip("Cant release flaps above the maximum release speed", 5, nil);
          else
            controls.flapsDown(1);
        </script>
      </binding>
    </action>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>BR_LOD_100</object-name>
    <property>/controls/flight/flaps</property>
    <offset-deg>0</offset-deg>
    <factor>14</factor>
    <center>
      <x-m>-0.74009</x-m>
      <y-m>0.26095</y-m>
      <z-m>0.08167</z-m>
    </center>
    <axis>
      <z>1</z>
      <x>.2</x>
    </axis>
  </animation>
